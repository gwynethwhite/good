    <div class="login">
      <div class="container">
        <div class="form">
          <class="login">
            <div class="container">
              <div class="form">

                <h1>Log in</h1>

                <%= form_for(:session, url: login_path) do |f| %>
                <%= f.email_field :email, :placeholder => "Email" %>
                <%= f.password_field :password, :placeholder => "Password" %>
                <%= f.submit "Log in", class: "btn-submit" %>
                <% end %>

                <a href="http://localhost:3000/signup">Sign up</a>

              </div>
            </div>
          </div>
